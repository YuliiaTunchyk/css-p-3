<html>

  <head>
    <title>
      Dashboard page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>
    <div class='page page--dashboard'>
      <aside
        class='page__aside aside vertical vertical--big'
      >
        <div class='text aside__logo'>
          Sidebar
        </div>
        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/home.svg')"
            ></span>
            <span class='text text--light'>Home</span>
          </a>
          <a
            href='#'
            class='nav-button nav-button--inactive click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/orders.svg')"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>
          <a
            href='#'
            class='nav-button nav-button--inactive click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/products.svg')"
            ></span>
            <span class='text text--light'>Products</span>
          </a>
        </nav>
      </aside>
      <header class='header'>
        <div class='layout header__content text--small'>
          <div class='header__search'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style="background-image: url('/svg/header/search.svg')"
            ></span>
            <span
              class='icon header__icon-settings click'
              style="background-image: url('/svg/header/settings.svg')"
            ></span>
          </div>
          <div class='layout header__icon--list'>
            <span
              class='icon header__icon--list icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/4405.svg')"
            ></span>
            <span
              class='icon header__icon--list icon--button icon--color-blue click'
              style="background-image: url('/svg/header/4406.svg')"
            ></span>
            <span
              class='icon header__icon--list icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/4407.svg')"
            ></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div
              class='analytic analytic--double analytic--purple'
            >
              <span
                class='icon analytic__button icon--button icon--color-purple click'
                style="background-image: url('/svg/analytic/4411.svg')"
              ></span>
              <div class='vertical vertical--small'>
                <div
                  class='analytic__title text text--light'
                >$500.00</div>
              </div>
              <div class='text text--small text--gray'>Total
                earning</div>
              <span
                class='icon analytic__arrow icon--button icon--color-purple click'
                style="background-image: url('/svg/analytic/arrow.svg')"
              ></span>
            </div>
            <div
              class='analytic analytic--double analytic--blue'
            ><span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/4413.svg')"
              ></span>
              <div class='vertical vertical--small'>
                <div
                  class='analytic__title text text--light'
                >$961</div>
              </div>
              <div class='text text--small text--gray'>Total
                Order</div>
            </div>
            <div>
              <div
                class='analytic analytic--double analytic--row analytic--blue'
              >
                <span
                  class='icon analytic__button icon--button icon--color-blue click'
                  style="background-image: url('/svg/analytic/4414.svg')"
                ></span>
                <div>
                  <div>
                    <div
                      class='analytic__content text text--bold'
                    >$203k</div>
                  </div>
                  <div
                    class='text text--small text--gray'
                  >Total Income</div>
                </div>
              </div>
              <div
                class='analytic analytic--double analytic--row analytic--bald'
              >
                <span
                  class='icon analytic__button icon--button icon--color-yellow click'
                  style="background-image: url('/svg/analytic/4415.svg')"
                ></span>
                <div>
                  <div>
                    <div
                      class='analytic__content text text--bold'
                    >$203k</div>
                  </div>
                  <div class='text text--small'>Total Income</div>
                </div>
              </div>
            </div>
          </section>
          <main class='checkout'>
            <div class='main__double'>
              <h2
                class='purchase__title text text--bold'
              >Purchase list</h2>
              <hr />
              <ul class='list-group text'>
                <li class='list-group-item'>ID437661</li>
                <li class='list-group-item'>AirOn ProCam</li>
                <li class='list-group-item'>132.32 EUR</li>
                <li class='list-group-item'>Customer</li>
                <li class='list-group-item'>May 11, 2023,
                  14:21</li>
                <li class='list-group-item'>7/8 USB Type-C</li>
                <li
                  class='list-group-item text text--warning'
                >Not paid</li>
                <li
                  class='list-group-item'
                >robin@com.de</li>
              </ul>
              <hr />
              <ul class='list-group text'>
                <li class='list-group-item'>ID437661</li>
                <li class='list-group-item'>PHILIPS
                  Essential</li>
                <li class='list-group-item'>543 USD</li>
                <li class='list-group-item'>Customer</li>
                <li class='list-group-item'>May 11, 2023,
                  14:21</li>
                <li class='list-group-item'>HD9200/90</li>
                <li
                  class='list-group-item text text--success'
                >Pay by Stripe</li>
                <li class='list-group-item'>dima@com.ua</li>
              </ul>
            </div>
            <div class='main__three'>
              <h2
                class='purchase__title text text--bold'
              >Payment</h2>
              <hr />
              <ul class='card-group vertical vertical--big'>
                <li class='list-group-item text'>
                  <input
                    class='form-check-input me-1'
                    type='radio'
                    name='listGroupRadio'
                    value=''
                    id='firstRadio'
                    checked
                  />
                  <label
                    class='form-check-label'
                    for='firstRadio'
                  >Credit card</label>
                </li>
                <li class='list-group-item text'>
                  <input
                    class='form-check-input me-1'
                    type='radio'
                    name='listGroupRadio'
                    value=''
                    id='secondRadio'
                  />
                  <label
                    class='form-check-label'
                    for='secondRadio'
                  >Debit card</label>
                </li>
                <li class='list-group-item text'>
                  <input
                    class='form-check-input me-1'
                    type='radio'
                    name='listGroupRadio'
                    value=''
                    id='thirdRadio'
                  />
                  <label
                    class='form-check-label'
                    for='thirdRadio'
                  >PayPal</label>
                </li>
              </ul>
              <nav class='container-fluid'>
                <div class='form'>
                  <div class='form-control'>
                    <h3 class='text text--dark'>Name on card</h3>
                    <br />
                    <div class='field'>
                    </div>
                  </div>
                  <div class='form-control'>
                    <h3 class='text text--dark'>Credit card
                      number</h3>
                    <br />
                    <div class='field'>
                    </div>
                  </div>
                </div>
                <div class='form'>
                  <div class='form-control'>
                    <h3
                      class='text text--dark'
                    >Expiration</h3>
                    <br />
                    <div class='field'>
                    </div>
                  </div>
                  <div class='form-control'>
                    <h3 class='text text--dark'>CVV</h3>
                    <br />
                    <div class='field'>
                    </div>
                  </div>
                </div>
                <hr />
                <div class='form'>
                  <div class='form-control'>
                    <h3 class='text text--dark'>Country</h3>
                    <br />
                    <div class='field click'>
                      <span
                        class='text text--dark'
                      >Choose...</span>
                      <select
                        class='field--select click'
                        name=''
                        id=''
                      ></select>
                    </div>
                  </div>
                  <div class='form-control'>
                    <h3 class='text text--dark'>State</h3>
                    <br />
                    <div class='field click'>
                      <span
                        class='text text--dark'
                      >Choose...</span>
                      <select
                        class='field--select click'
                        name=''
                        id=''
                      ></select>
                    </div>
                  </div>
                </div>
                <hr />
                <a
                  href='#'
                  class='nav-button nav-button--active2 click'
                >
                  <span class='text text--light'>Continue to
                    checkout</span>
                </a>
              </nav>
            </div>
          </main>
        </div>
      </main>
    </div>
  </body>
</html>